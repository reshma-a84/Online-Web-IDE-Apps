{
	"_version": "1.5.0",
	"sap.app": {
		"id": "ProductNavigation",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.38.11"
		},
		"dataSources": {
			"backendData": {
				"uri": "/destination/Northwind/V3/Northwind/Northwind.svc",
				"type": "OData"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ProductNavigation.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ProductNavigation.i18n.i18n"
				}
			},
			"": {
				"dataSource": "backendData"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"viewPath": "ProductNavigation.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"routerClass": "sap.m.routing.Router"
			},
			"routes": [{
				"name": "home",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["home"]
			}, {
				"name": "productDetail",
				"pattern": "product",
				"titleTarget": "",
				"greedy": false,
				"target": ["prod_detail"]
			}],
			"targets": {
				"home": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "View1",
					"viewLevel": 1
				},
				"prod_detail": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": "true",
					"viewName": "View2",
					"viewLevel": 2
				}
			}
		}
	}
}