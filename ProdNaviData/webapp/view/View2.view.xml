<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="ProdNaviData.controller.View2">
	<App>
		<pages>
			<Page title="Title" showNavButton="true" navButtonPress="handleNavButtonPress">
				<content>
				    <VBox width="100%" direction="Column" id="__vbox0">
				    <items>
				        <Text text="{ProductName}" id="__text0"/>
				        <Text text="{QuantityPerUnit}" id="__text3"/>
				        <Text text="{UnitPrice}" id="__text2"/>
				        <Text text="{UnitsInStock}" id="__text1"/>
				    </items>
				    </VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>