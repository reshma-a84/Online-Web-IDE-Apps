<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ProdNaviData.controller.View1" displayBlock="true">
	<App>
		<pages>
			<Page title="{i18n>title}">
			<content>
			    <List items="{/Products}" id="__list0">
			     
			            <StandardListItem type="Navigation" title="{ProductName}" icon="sap-icon://picture" id="__item0" press="handleListItemPress"/>
			           
			    </List>
			</content>
			</Page>
		</pages>
	</App>
</mvc:View>